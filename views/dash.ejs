        <div class="container">
            <div class="header">
                <h1>Welcome back <p><%= Owner[0].username %></p></h1>
                <h3>Time of refresh: <% var now = new Date(); %><%= now.getHours() - 6 %>:<%= (now.getMinutes() < 10? '0' : '') + now.getMinutes();%></h3>
            </div>
            <div class="row messages-container">
            
                <% for( var i = customer.length -1; i >= 0; i-- ){%>
                    <div class="col-lg-7 col-md-8 col-sm-10 col-12 message">
                        <div class="name-cont">
                            <p id="name"><%= customer[i].name %></p>
                            <div class="email-cont">
                                <p id="email"><%= customer[i].email %></p>
                            </div>
                        </div>
                        <div class="message-cont">
                            <p><%= customer[i].message %></p>
                        </div>
                        <span><%=customer[i].date.hour%>: <%=customer[i].date.minute%>  <i><%= customer[i].date.month %>/<%= customer[i].date.day %>/<%= customer[i].date.year %></i></span>
                    </div>
                    
                <% } %>
            </div>
        </div>
        